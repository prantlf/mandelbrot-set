  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "update-bytesize-icons",
      "program": "${workspaceFolder}/scripts/update-bytesize-icons"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "update-mini-css",
      "program": "${workspaceFolder}/scripts/update-mini-css"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server",
      "program": "${workspaceFolder}/scripts/server"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "mandelbrot",
      "url": "http://localhost:8008/mandelbrot.html",
      "webRoot": "${workspaceFolder}/public",
      "preLaunchTask": "server"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "julia",
      "url": "http://localhost:8008/julia.html",
      "webRoot": "${workspaceFolder}/public",
      "preLaunchTask": "server"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "demo",
      "url": "http://localhost:8008/demo.html",
      "webRoot": "${workspaceFolder}/public",
      "preLaunchTask": "server"
    }
  ]
}
