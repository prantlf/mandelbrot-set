  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "update-bytesize-icons",
      "program": "${workspaceFolder}/scripts/update-bytesize-icons"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "update-mini-css",
      "program": "${workspaceFolder}/scripts/update-mini-css"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:8008/mandelbrot.html",
      "webRoot": "${workspaceFolder}/pages",
      "preLaunchTask": "Run Server"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Attach Chrome",
      "url": "http://localhost:8008/mandelbrot.html",
      "webRoot": "${workspaceFolder}/pages"
    }
  ]
}
